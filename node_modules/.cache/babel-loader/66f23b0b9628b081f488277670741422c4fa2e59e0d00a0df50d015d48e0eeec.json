{"ast":null,"code":"// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from 'swiper/vue';\n\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/pagination';\nimport 'swiper/css/navigation';\n\n// import required modules\nimport { Autoplay, Pagination, Navigation } from 'swiper/modules';\nimport Button from '@/components/v-button';\nexport default {\n  name: 'v-card-gallery',\n  components: {\n    Swiper,\n    SwiperSlide,\n    Button\n  },\n  setup() {\n    return {\n      modules: [Autoplay, Pagination, Navigation]\n    };\n  }\n};","map":{"version":3,"names":["Swiper","SwiperSlide","Autoplay","Pagination","Navigation","Button","name","components","setup","modules"],"sources":["/home/vitalii/projects/klinika/src/components/v-main-gallery.vue"],"sourcesContent":["<template>\n  <swiper\n    :spaceBetween=\"0\"\n    :centeredSlides=\"true\"\n    :pagination=\"{\n      clickable: true,\n    }\"\n    :navigation=\"true\"\n    :modules=\"modules\"\n    class=\"mySwiper\"\n  >\n    <swiper-slide>\n      <img src=\"../images/main-gallery/Image1.jpg\" class=\"card-gallery\">\n      <Button class=\"route-button\" text=\"Подробнее\"/>\n    </swiper-slide>\n    <swiper-slide>\n      <img src=\"../images/main-gallery/Image2.jpg\" class=\"card-gallery\">\n      <router-link :to=\"{ name: 'Service', hash: '#head'}\"> \n        <Button class=\"route-button\" text=\"Подробнее\"/>\n      </router-link>\n    </swiper-slide>\n    <swiper-slide><img src=\"../images/main-gallery/Image1.jpg\" class=\"card-gallery\"></swiper-slide>\n  </swiper>\n</template>\n<script>\n  // Import Swiper Vue.js components\n  import { Swiper, SwiperSlide } from 'swiper/vue';\n\n  // Import Swiper styles\n  import 'swiper/css';\n\n  import 'swiper/css/pagination';\n  import 'swiper/css/navigation';\n\n\n  // import required modules\n  import { Autoplay, Pagination, Navigation } from 'swiper/modules';\n  import Button from '@/components/v-button'\n\n  export default {\n    name: 'v-card-gallery',\n    components: {\n      Swiper,\n      SwiperSlide,\n      Button,\n    },\n    setup() {\n      return {\n        modules: [Autoplay, Pagination, Navigation],\n      };\n    },\n  };\n</script>\n\n<style>\n.route-button {\n  position: absolute;\n  left: 50%;\n  top: 80%;\n  transform: translate(-50%,-50%);\n}\n\n.card-gallery {\n  width: 100%;\n  aspect-ratio: 16 / 7;\n  object-fit: cover;\n}\n.swiper-pagination-bullet, .swiper-pagination-bullet-active {\n  background-color: #121820;\n  width: 13px;\n  height:13px;\n}\n.swiper-button-next:after, .swiper-container-rtl .swiper-button-prev:after {\n  color: #121820;\n}\n.swiper-button-next:hover {\n  color: white;\n  opacity:0.4;\n}\n\n.swiper-button-prev:after, .swiper-container-rtl .swiper-button-prev:after {\n  color: #121820;\n}\n.swiper-button-prev:hover {\n  color: white;\n  opacity:0.4;\n}\n\n@media screen and (max-width: 700px) {\n  .card-gallery {\n    aspect-ratio: 3 / 3;\n    object-fit: cover;\n  }\n}\n</style>\n"],"mappings":"AAyBE;AACA,SAASA,MAAM,EAAEC,WAAU,QAAS,YAAY;;AAEhD;AACA,OAAO,YAAY;AAEnB,OAAO,uBAAuB;AAC9B,OAAO,uBAAuB;;AAG9B;AACA,SAASC,QAAQ,EAAEC,UAAU,EAAEC,UAAS,QAAS,gBAAgB;AACjE,OAAOC,MAAK,MAAO,uBAAsB;AAEzC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVP,MAAM;IACNC,WAAW;IACXI;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,OAAO;MACLC,OAAO,EAAE,CAACP,QAAQ,EAAEC,UAAU,EAAEC,UAAU;IAC5C,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}